#!/bin/bash
set -o errexit
set -o nounset
set -o pipefail

echo ""
echo "#############################################"
echo "🌐 Starting WebFrontend"

echo "" > /tmp/healthcheck
echo "php-fpm" >> /tmp/healthcheck # Process name is `php-fpm`, not `php-fpm8.4`
echo "nginx" >> /tmp/healthcheck

php-fpm8.4 -D && nginx -g "daemon off;"